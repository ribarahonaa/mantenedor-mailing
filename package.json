{
  "name": "mantenedor-mailing-backend",
  "version": "1.0.0",
  "description": "Backend para el mantenedor de mailings con Node.js, Express y PostgreSQL",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "init-db": "node init-database.js",
